<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <noscript><meta http-equiv="refresh" content="0; URL=/nojs.html" /></noscript>
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/bundles/js/base")
    @Scripts.Render("~/bundles/js/template-editor")
    @RenderSection("CustomHeaderScripts", required: false)
    @Styles.Render("~/bundles/base")
    @RenderSection("CustomStyles", required: false)
    @RenderSection("PageSpecificLinks", required: false)
    <script></script>
</head>
<body style="margin:20px;">

        @Html.Partial("_Header")
    <div class="container">
        @RenderBody()
    </div>
        @Html.Partial("_Footer")

    <!--[if lt IE 9]>
         <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
         <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
       <![endif]-->
    @RenderSection("CustomBodyScripts", required: false)
    @RenderSection("scripts", required: false)
</body>
</html>
