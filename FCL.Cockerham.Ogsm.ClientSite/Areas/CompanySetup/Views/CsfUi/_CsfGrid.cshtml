<div class="box box-info">
    <div class="box-header with-border">
        <h3 class="box-title">Csf List</h3>
    </div>
    <div id="gridContent" class="box-body">
        <table id="tblCsfs" class="table table-bordered table-striped" ng-table="csfVm.tableParams">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Description</th>
                    <th>Strategy Type</th>
                    <th>Active</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="csf in $data">
                    <td data-title="'Name'">{{csf.Name}}</td>
                    <td data-title="'Description'">{{csf.Description}}</td>
                    <td data-title="'St Type'">{{csf.StType.Name}}</td>
                    <td data-title="'Is Active'">{{csf.IsActive ? "Yes":"No"}}</td>
                    <td data-title="'Edit'">
                        <button type="button"
                                class="btn btn-default btn-xs pull-right"
                                ng-click="LoadById(csf.CsfId)">
                            <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
                        </button>
                    </td>
                </tr>
            </tbody>
            <tfoot></tfoot>
        </table>
        @Resources.SystemResources.Common_webgrid_row_count_text <span id="rowCount">{{csfVm.tableParams.total()}}</span>
    </div>
</div>