@model FCL.Cockerham.Ogsm.Entities.DTOs.ActionPlanDto

@{
    ViewBag.Title = "Stratus OGSM - Client Portal Action Plan Setup - Action Plan";
}

<script src="~/Scripts/App/ActionPlanSetup/ActionPlan/ActionPlan.js"></script>

<section class="content-header">
    <h1>
        Action Plan Setup - Action Plan
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Dashboard</a></li>
        <li class="active">Action Plan Setup - Action Plan</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="col-lg-12 ">
        <div>
            <div ng-controller="ActionPlanController as ctrl">
                <div class="row">
                    <div class="col-md-12">
                        <div class="box box-success">
                            @Html.Partial("_CreateActionPlan", @Model)
                        </div>
                    </div>
                    <div class="col-md-12">
                        @Html.Partial("_ActionPlanGrid")
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- /.content -->
